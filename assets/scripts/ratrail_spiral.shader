ratRailSpiralLightning0
 {
 	nopicmip
 	nomipmaps
 	sort nearest
 	cull disable
 	//deformVertexes autosprite2
 	{
 		map gfx/misc/ratrail/lspiral0.tga
 		//tcMod transform 51 0 0 1 0 0
 		tcMod transform 25 0 0 1 0 0
 		blendFunc add
 		rgbGen entity
 	}
 	{
 		map gfx/misc/ratrail/lspiral0.tga
 		//tcMod transform 51 0 0 1 0 0
 		tcMod transform 25 0 0 1 0 0
 		blendFunc add
 		//rgbGen wave inversesawtooth 0 0.5 0 1.6
 		//rgbGen wave sin 0 0.5 0.25 0.4
 		rgbGen wave sin 0 0.25 0.25 0.7
 	}
 }

